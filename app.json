{
  "expo": {
    "name": "TaraG",
    "slug": "TaraG",
    "owner": "revieqt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tarag",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.revieqt.tarag",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "192.168.1.57": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0"
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.revieqt.tarag",
      "useNextNotificationsApi": true,
      "networkSecurityConfig": {
        "domain-config": [
          {
            "domain": "192.168.1.57",
            "cleartextTrafficPermitted": true
          }
        ]
      }
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      ["react-native-google-mobile-ads", 
        { "androidAppId": "ca-app-pub-8325794315541816~1418527751",
          "iosAppId": "ca-app-pub-8325794315541816~2492770428"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "sounds": ["./assets/notification-sound.wav"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0f5f4176-b24d-4bab-b0d1-846b25758c3d"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/0f5f4176-b24d-4bab-b0d1-846b25758c3d"
    }
  }
}
